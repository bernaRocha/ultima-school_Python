    "import turtle\n",
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('black')\n",
    "t.pencolor('red')\n",
    "t.speed(0)\n",
    "for i in range(150):\n",
    "    t.circle(190 - 1, 90)\n",
    "    t.lt(98)\n",
    "    t.circle(190 - 1, 90)\n",
    "    t.lt(18)\n",
    "s.exitonclick() \n"
   ]
  }
