    "import turtle\n",
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('#262626')\n",
    "t.width(2)\n",
    "t.speed(15)\n",
    "\n",
    "col = ('white', 'pink', 'cyan')\n",
    "for i in range(300):\n",
    "    t.pencolor(col[i%3])\n",
    "    t.forward(i*4)\n",
    "    t.right(121)\n",
    "s.exitonclick()"
   ]
